import "hashes/mimc7/mimc7R10"

def main (private field birthday, field delta, field today, private field photo_hash, private field private_key) -> field:
    assert(birthday + delta < today)
    field k = birthday * private_key
    field public_key = mimc7R10(photo_hash, k) * photo_hash
    return mimc7R10(today, public_key)

  